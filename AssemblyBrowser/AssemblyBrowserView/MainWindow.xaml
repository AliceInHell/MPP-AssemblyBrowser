<Window x:Class="AssemblyBrowserView.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssemblyBrowserLibrary"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="SelectAssembly" Content="Show" Margin="378,274,55,0" Command="{Binding OpenCommand}" RenderTransformOrigin="0.493,0.478" VerticalAlignment="Top"/>

        <TextBox Text="{Binding OutputString}" Margin="0,10,0,50"/>
    </Grid>
</Window>
